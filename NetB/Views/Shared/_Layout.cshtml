<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Net.B</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/jquery-3.1.0.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</head>
<body>
    <div class="nav navbar-inverse">
        <img src="#" width="50" height="50" class="img-circle navbar-brand" />
        <a class="navbar-brand" href="#">Net.B</a>
    </div>
    <div class="container">
        @RenderBody()
    </div>
    <footer class="nav navbar-inverse navbar-fixed-bottom panel-footer">
        <p>&copy; @DateTime.Now.Year - Net.B 2016</p>
    </footer>
</body>
</html>