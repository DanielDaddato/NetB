@using Newtonsoft.Json
@{
    Layout = null;
    ViewBag.Title = "NetB - Login";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/jquery-3.1.0.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/scripts/Navegacao/UpdatePanel.js"></script>
    <script src="~/scripts/Navegacao/Navegacao.js"></script>
</head>
<body>
    <div class="nav navbar-inverse" style="height:100px;">
        <img src="~/Content/images/Netb/netb.jpg" style="height:75px; width:75px;" class="img-rounded navbar-brand" />
        <a class="navbar-brand" style="font-size:50px; margin-top: 14px;" href="#">Net.B</a>
    </div>
    <div id="container" class="container">
        <div class="panel panel-default" style="margin-top: 200px; margin-left: 200px;width: 600px;">
            <div class="panel-heading">
                <h3 class="panel-title">Login</h3>
            </div>
            <div class="panel-body center-block">
                <input id="login" placeholder="Email" type="text" class="input-sm center-block " />
                <input id="senha" placeholder="Senha" type="password" class="input-sm center-block" />
                <input type="button"class="center-block btn btn-primary" value="Entrar" onclick="LoginIndex();" />
            </div>
        </div>
    </div>
    <footer class="nav navbar-inverse navbar-fixed-bottom panel-footer">
        <p>&copy; @DateTime.Now.ToString() - Net.B 2016</p>
    </footer>
</body>
</html>
